<!Doctype html>

<html>

<head>

<title><%=pageTitle%></title>

<meta charset="utf-8">
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
<link rel="stylesheet" href="../public/css/style1.css"/>

<script src="https://kit.fontawesome.com/yourcode.js"></script>


</head>

<body>
    <%-include('./partials/menu')%>
    <div class="container-fluid">
        <div class="row">
             <div class="col-12" id="picTitle">
              <h1><%=pageTitle%></h1>
            </div>
        </div>
        <div class="row" id="Contenu">
            <div class="col-xs-12 col-sm-12 col-xl-12 d-block">
                <form>
                    <div class="form-row">
                      <div class="form-group col-md-6">
                        <label for="inputEmail4">Nom</label>
                        <input type="text" class="form-control" id="inputEmail4" placeholder="Nom">
                      </div>
                      <div class="form-group col-md-6">
                        <label for="inputPassword4">Prenom</label>
                        <input type="text" class="form-control" id="inputPassword4" placeholder="Prenom">
                      </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                          <label for="inputEmail4">Email</label>
                          <input type="text" class="form-control" id="inputEmail4" placeholder="Email">
                        </div>
                        <div class="form-group col-md-6">
                          <label for="inputPassword4">Telephone</label>
                          <input type="text" class="form-control"  placeholder="Telephone">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="comment">Message:</label>
                        <textarea class="form-control" rows="5" id="comment" placeholder="Message"></textarea>
                      </div> 
                    <button type="submit" class="btn">Envoyer</button>
                  </form>
            </div>  
        </div>
    </div> 
    <%-include('./partials/footer')%>
</body>

</html>

